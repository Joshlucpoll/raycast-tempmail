{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tempmail",
  "title": "TempMail",
  "description": "10 minute mail using the mail.gw API",
  "icon": "command-icon.png",
  "author": "Joshlucpoll",
  "categories": [
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "tempmail",
      "title": "TempMail",
      "subtitle": "Open temporary mailbox",
      "description": "",
      "mode": "view",
      "preferences": [
        {
          "name": "expiry_time",
          "description": "Email will expiry (a new one will be generated) after a set period of inactivity. If set to \"Don't expire\", emails can only be regenerated manually",
          "type": "dropdown",
          "required": true,
          "title": "Email Expiry Time",
          "default": "10",
          "data": [
            {
              "title": "Don't expire",
              "value": "null"
            },
            {
              "title": "5 minutes",
              "value": "5"
            },
            {
              "title": "10 minutes",
              "value": "10"
            },
            {
              "title": "30 minutes",
              "value": "30"
            },
            {
              "title": "1 hour",
              "value": "60"
            },
            {
              "title": "12 hour",
              "value": "720"
            },
            {
              "title": "1 day",
              "value": "1440"
            },
            {
              "title": "1 week",
              "value": "10080"
            }
          ]
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.9",
    "@raycast/utils": "^1.5.2",
    "axios": "^1.3.4",
    "eml-parser": "^1.2.0",
    "moment": "^2.29.4",
    "turndown": "^7.1.2",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/turndown": "^5.0.1",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
